# Copy this file to .env and fill in your API keys

# Google API Key for Gemini Embeddings
GOOGLE_API_KEY=your_google_api_key_here

# Pinecone API Key for Vector Store
PINECONE_API_KEY=your_pinecone_api_key_here

# Groq API Key for LLM
GROQ_API_KEY=your_groq_api_key_here

# LlamaCloud API Key for LlamaParse (PDF parsing with table/image extraction)
LLAMA_CLOUD_API_KEY=your_llamacloud_api_key_here

# Pinecone Environment (optional, default: us-east-1)
PINECONE_ENV=us-east-1

